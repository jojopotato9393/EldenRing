@model EldenRing.Models.Response

<h2>Items</h2>

<div style="display: flex; flex-wrap: wrap;">
    @foreach (Items item in Model.Data)
    {
         <div style="margin: 10px; text-align: center;"> 
            <img src="@item.ImageUrl" alt="@item.Name">
            <h3>@item.Name</h3>
        </div> 
    }
</div>

